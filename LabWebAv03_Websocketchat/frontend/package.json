{
  "name": "websocket-chat-frontend",
  "version": "1.0.0",
  "description": "Frontend server for WebSocket chat application",
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node index.ts",
    "dev:watch": "nodemon --watch . --ext ts,html,js --exec ts-node index.ts",
    "start": "ts-node index.ts",
    "build": "tsc",
    "serve": "node dist/index.js"
  },
  "dependencies": {
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "websocket",
    "chat",
    "frontend",
    "express"
  ],
